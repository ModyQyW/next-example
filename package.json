{
  "name": "next-example",
  "private": true,
  "scripts": {
    "build": "next build",
    "commit": "git-cz",
    "dev": "next dev",
    "lint": "pnpm run lint:eslint && pnpm run lint:markdownlint && pnpm run lint:stylelint && next lint",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue,.svelte --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue,svelte}\" --fix --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || husky install",
    "start": "next start"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@emotion/react": "~11.7.1",
    "@emotion/styled": "~11.6.0",
    "@fontsource/roboto": "~4.5.1",
    "@iconify/react": "~3.1.2",
    "@mui/icons-material": "~5.2.5",
    "@mui/material": "~5.2.8",
    "next": "~12.0.7",
    "react": "~17.0.2",
    "react-dom": "~17.0.2"
  },
  "devDependencies": {
    "@babel/core": "~7.16.7",
    "@babel/eslint-parser": "~7.16.5",
    "@commitlint/cli": "~16.0.2",
    "@modyqyw/fabric": "~4.7.3",
    "@types/react": "~17.0.38",
    "@typescript-eslint/eslint-plugin": "~5.9.1",
    "@typescript-eslint/parser": "~5.9.1",
    "commitizen": "~4.2.4",
    "eslint": "~8.6.0",
    "husky": "~7.0.4",
    "is-ci": "~3.0.1",
    "lint-staged": "~12.1.7",
    "markdownlint-cli": "~0.30.0",
    "postcss": "~8.4.5",
    "postcss-flexbugs-fixes": "~5.0.2",
    "postcss-preset-env": "~7.2.0",
    "prettier": "~2.5.1",
    "stylelint": "~14.2.0",
    "tailwindcss": "~3.0.12",
    "typescript": "~4.5.4"
  }
}
